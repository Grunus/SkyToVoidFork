var DodgeIntensity = 1
var TickDodgeChance = 0.9 //per tick
var DistanceWithinToDodge = 10

//This section is for dodging if the player is just LOOKING at the npc
function tick(t){
if(t.npc.isAttacking()){
var p = t.npc.getAttackTarget()
if(p.rayTraceEntities(DistanceWithinToDodge,false,false).length > 0 && p.rayTraceEntities(10,false,false)[0] == t.npc && Math.random() < TickDodgeChance){
Dodge(t.npc,p)}}}

/* Damaged function trigger if you dont want to run tick function 
var DodgeChance = 0.5
function damaged(t){
if(t.npc.isAttacking() && t.source && Math.random() < DodgeChance){
t.setCanceled(true)
Dodge(t.npc,t.source)}}
*/

function Dodge(npc,targ){
npc.world.playSoundAt(npc.getPos(),\"minecraft:entity.blaze.shoot\",1,1.3)
var d = FrontVectors(npc,GetPlayerRotation(npc,targ)+90*RandSign(),0,DodgeIntensity,0)
npc.setMotionX(d[0])
npc.setMotionY(0.2)
npc.setMotionZ(d[2])}

function FrontVectors(entity,dr,dp,distance,mode){
if(!mode)mode=0
if(mode == 1){var angle = dr + entity.getRotation();var pitch = (-entity.getPitch()+dp)*Math.PI/180;if(dp==0)pitch=0;}
if(mode == 0){var angle = dr;var pitch = (dp)*Math.PI/180}
var dx = -Math.sin(angle*Math.PI/180)*(distance*Math.cos(pitch))
var dy = Math.sin(pitch)*distance
var dz = Math.cos(angle*Math.PI/180)*(distance*Math.cos(pitch))
return [dx,dy,dz]}

function GetPlayerRotation(npc, player){
var dx = npc.getX()-player.getX();
var dz = player.getZ()-npc.getZ();
if(dz>=0){
var angle = (Math.atan(dx/dz)*180/Math.PI);
if(angle < 0){
angle = 360+angle;}}
if(dz<0){
dz = -dz;
var angle = 180-(Math.atan(dx/dz)*180/Math.PI);}
return angle;}

function RandSign(){
var s = Math.floor(Math.random()*2)
if(s==0)s=-1;
return s;}

