    "Scripts": [
        {
            "Script": "//As is with any script that spawns entities, there is a chance for crashes

var Projectile = \"minecraft:arrow\"
var ShotCount = 12
var JumpIntensity = 1.2
var JumpToArrowDelay = 15 //ticks
var AbilityAttemptTime = 8 //ticks
var Accuracy = 7
var DistanceCheck = 3 //-1 to disable
var Cooldown = 3 //seconds
var T = [1,2,3] //Timer IDs used in this script

function init(t){
t.npc.timers.forceStart(T[0],AbilityAttemptTime,true)}

function timer(t){
if(t.id==T[0]){
if(t.npc.isAttacking() && t.npc.isAlive() && (DistanceCheck == -1 || t.npc.getPos().distanceTo(t.npc.getAttackTarget().getPos()) <= DistanceCheck) && !t.npc.timers.has(T[2])){
t.npc.timers.forceStart(T[2],Cooldown*20,false)
JumpAndArrows(t.npc)}}
if(t.id == T[1]){
var targ = t.npc.getAttackTarget()
if(targ == null)return;
t.npc.swingMainhand()
t.npc.world.playSoundAt(t.npc.getPos(),t.npc.stats.ranged.getSound(0),3,1)
for(var i = 0;i<ShotCount;++i){
t.npc.shootItem(targ,t.npc.world.createItem(Projectile,0,1),5)}}}

function JumpAndArrows(npc){
npc.knockback(1,npc.getRotation()-180)
npc.setMotionY(JumpIntensity)
npc.world.playSoundAt(npc.getPos(),\"minecraft:entity.blaze.shoot\",1,1)
npc.timers.forceStart(T[1],JumpToArrowDelay,false)}




//end",
